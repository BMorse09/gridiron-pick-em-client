<h2> Week 4 Matchups</h2>
<h3> My Picks!</h3>

{{#each predictions}}

  <div>
    {{matchup.away}}
    @
    {{matchup.home}}
    :
    {{pick}}
  </div>
  <button class="btn btn-default edit-pick-button" data-id={{id}}> Edit Pick </button>
{{/each}}


